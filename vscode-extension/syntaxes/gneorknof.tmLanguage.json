{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "gneorknof",
  "patterns": [
    {
      "include": "#keywords"
    },
    {
      "include": "#numbers"
    },
    {
      "include": "#comments"
    },
    {
      "include": "#functions"
    },
    {
      "include": "#variables"
    },
    {
      "include": "#strings"
    }
  ],
  "repository": {
    "keywords": {
      "patterns": [
        {
          "name": "keyword.control.gneorknof",
          "match": "\\b(if|while|for|return|else|as|do|import|export|from|await|break|continue)\\b"
        },
        {
          "name": "keyword.other.gneorknof",
          "match": "\\b(func|mut|var|const|struct|trait|impl|true|false|async|type)\\b"
        }
      ]
    },
    "numbers": {
      "name": "constant.numeric.gneorknof",
      "match": "\\d+(\\.\\d+)?"
    },
    "comments": {
      "patterns": [
        {
          "name": "comment.line.number-sign.gneorknof",
          "match": "#.*$"
        }
      ]
    },
    "functions": {
      "patterns": [
        {
          "name": "entity.name.function.gneorknof",
          "match": "\\b[a-z_][a-zA-Z0-9_]*(?=\\()"
        }
      ]
    },
    "variables": {
      "patterns": [
        {
          "name": "entity.name.type.gneorknof",
          "match": "\\b[A-Z_][a-zA-Z0-9_]*\\b"
        },
        {
          "name": "variable.other.gneorknof",
          "match": "\\b[a-zA-Z_][a-zA-Z0-9_]*\\b"
        }
      ]
    },
    "strings": {
      "name": "string.quoted.double.gneorknof",
      "begin": "\"",
      "end": "\"",
      "patterns": [
        {
          "name": "constant.character.escape.gneorknof",
          "match": "\\\\."
        }
      ]
    }
  },
  "scopeName": "source.gneorknof"
}
